<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>nieltg</Authors>
    <Company />
    <Description>A porting of ReadSharp library for .NET Standard 1.6 platform</Description>
    <Version>1.0.3</Version>
    <FileVersion>1.0.3.0</FileVersion>
    <AssemblyVersion>1.0.3.0</AssemblyVersion>
    <PackageProjectUrl>https://github.com/nieltg/ReadSharp.Core</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nieltg/ReadSharp.Core</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/nieltg/ReadSharp.Core/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>2017</Copyright>
    <RepositoryType></RepositoryType>
    <RootNamespace>ReadSharp</RootNamespace>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.5.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ReadSharp.Ports.NReadability.Core\ReadSharp.Ports.NReadability.Core.csproj" />
  </ItemGroup>

</Project>
